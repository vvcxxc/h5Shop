.user_button {
  text-align: center;
  font-size: 36px;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

ul {
  list-style: none;
  padding: 0;
  margin: 0;
}

.giving {
  position: absolute;
  right: 12px;
  bottom: 20px;
  width: 53px;
  height: 53px;
  line-height: 53px;
  text-align: center;
  background: linear-gradient(0deg, rgba(243, 94, 4, 1) 0%, rgba(255, 163, 58, 1) 100%);
  box-shadow: 0px 8px 7px 0px rgba(6, 0, 1, 0.08);
  border-radius: 50%;
  font-size: 28px;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

