        background-repeat: no-repeat;
        .small-box {
          display: flex;
          justify-content: space-between;
          align-items: center;
          width 300px;
          .price-box {
            .symbol {
              margin-left 5px;
              font-size:17px;
              font-family:PingFang SC;
              font-weight:500;
              color:rgba(195,36,41,1);
            }
            .price {
              font-size:43px;
              font-family:Adobe Heiti Std;
              font-weight:normal;
              color:rgba(195,36,41,1);
            }
          }
        }
        

        .doorsill-box {
          display: flex;
          flex-direction:column;
          align-items: center;
          width: 120px;
          .type {
            font-size:19px;
            font-family:PingFang SC;
            font-weight:800;
            color:rgba(195,36,41,1);
          }
          .doorsill {
            font-size:14px;
            font-family:PingFang SC;
            font-weight:800;
            color:rgba(195,36,41,1);
          }
        }

      }


    }

    .gift-img-fourth-line {
      margin-top 38px;
      display: flex;
      justify-content: center;
      align-items: center;
      width:360px;
      height:33px;
      font-size:17px;
      font-family:PingFang SC;
      font-weight:500;
      color:rgba(255,255,255,1);
      border:1px solid rgba(255,255,255,1);
    }

  }
}


.info {
  margin: 0 26px;
  padding-top 21px;
  display: flex;
  flex-direction: column;
  align-items: center;
  border: 1px solid rgba(226, 226, 226, 1);
}

.gift_box {
  position: relative;
  display: flex;
  padding: 19px 13px;
  width: 350px;
  background: rgba(255, 255, 255, 1);
  border-radius: 20px;
  border: 2px solid #F5F2F5;

  .left {
    width: 121px;
    height: 112px;
    .taro-img {
      width: 120px;
      height: 112px;
    }
  }


  .right {
    padding-left: 5px;

    .price {
      color: rgba(238, 38, 51, 1);
    }

    .right-first-line {
      margin-bottom: 28px;
      font-size: 22px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(0, 0, 0, 1);
    }

    view {
      font-size: 16px;
      font-family: PingFang SC;
      font-weight: bold;

      text {
        font-size: 28px;
      }
    }

  }
}



