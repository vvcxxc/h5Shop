  .main {
    margin-top: 12px;
    margin-bottom: 28px;

    .gift_img {
      margin: 0px 26px;
      display: flex;
      flex-direction: column;
      align-items: center;
      height: 507px;
      text-align: center;
      background-color: red;

      .gift-img-first-line {
        padding-top 30px;
        .taro-img {
          height: 30px;
          width: auto;
        }
        .max_money {
          font-size:42px;
          font-family:PingFang SC;
          font-weight:800;
          color:rgba(255,255,255,1);
        }
      }
      .gift-img-second-line {
        .taro-img {
          height: 48px;
          width: 404px;
        }
        
      }


    .gift-img-third-line {
        display: flex;
        align-items: center;
        width 395px;
        height 140px;
        background: url("https://oss.tdianyi.com/front/BAHcaGjG55b68wnR3HfNQF6eDz7StdXz.png");
