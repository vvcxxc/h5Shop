.info-have-gift {
  margin: 0 26px;
  padding-top 20px;
  padding-bottom 10px;
  display: flex;
  flex-direction: column;
  align-items: center;
  border: 1px solid rgba(226, 226, 226, 1);
  .gift_box {
    position: relative;
    display: flex;
    padding: 19px 13px;
    width: 350px;
    background: rgba(255, 255, 255, 1);
    border-radius: 20px;
    border: 2px solid #F5F2F5;
    .giving {
      position: absolute;
      right: 12px;
      bottom: 20px;
      width: 53px;
      height: 53px;
      line-height: 53px;
      text-align: center;
      background: linear-gradient(0deg, rgba(243, 94, 4, 1) 0%, rgba(255, 163, 58, 1) 100%);
      box-shadow: 0px 8px 7px 0px rgba(6, 0, 1, 0.08);
      border-radius: 50%;
      font-size: 28px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(255, 255, 255, 1);
    }
    .left {
      width: 121px;
      height: 112px;
      .taro-img {
        width: 120px;
        height: 112px;
        border-radius: 10px;
        overflow hidden;
      }
    }


    .right {
      padding-left: 10px;

      .price {
        color: rgba(238, 38, 51, 1);
      }

      .right-first-line {
        margin-bottom: 28px;
        font-size: 22px;
        font-family: PingFang SC;
        font-weight: bold;
        color: rgba(0, 0, 0, 1);
      }

      view {
        font-size: 16px;
        font-family: PingFang SC;
        font-weight: bold;

        text {
          font-size: 28px;
        }
      }

    }
  }
}





