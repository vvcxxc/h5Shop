
  .spell-group-poster {
    width: 544px;
    height 880px
    background: rgba(255, 255, 255, 1);
    position absolute;
    z-index: -100;
    top: 0;
    left 0;

    .taro-img {
      width: 100%;
      height auto;
      img {
        height:auto;
      }
    }

    .spell-group-main {
      .gift-img {
        text-align: center;
        height: 507px;
        position: relative;

        .gift-image {//主图
          height 100%;
        }

        .give-gift {
          position: absolute;
          bottom: 20px;
          right: 40px;
          width: 192px;
          height: 208px;
          background: #EE2633;
          border-radius: 20px 21px;
          padding-bottom: 6px;

          .give-gift-first-line {
            padding: 12px 0px;
            font-size: 28px;
            font-family: PingFang SC;
            font-weight: 800;
            color: rgba(255, 255, 255, 1);
          }

         .give-gift-second-line {
            padding: 0px 12px;
            position: relative;

            .taro-img {//礼品图
              width: 170px;
              height: 136px;
              border-radius: 0px 0px 10px 10px;
            }
            
            img {
              width: 170px;
              height: 136px;
              border-radius: 0px 0px 10px 10px;
            }

            .test {
              position: absolute;
              top: 1px;
              left: 13px;
              border-radius: 0px;
            }

            .giving {
              position: absolute;
              right: 18px;
              top: 10px;
              width: 49px;
              height: 49px;
              line-height: 49px;
              text-align: center;
              background: linear-gradient(0deg, rgba(243, 94, 4, 1) 0%, rgba(255, 163, 58, 1) 100%);
              box-shadow: 0px 8px 7px 0px rgba(6, 0, 1, 0.08);
              border-radius: 50%;
              font-size: 28px;
              font-family: PingFang SC;
              font-weight: bold;
              color: rgba(255, 255, 255, 1);
            }

            .price-spell_group {
              position: absolute;
              left: 15px;
              bottom: 15px;
              font-size: 26px;
              font-family: PingFang SC;
              font-weight: 800;
              display: inline-block;
              width: 165px;
            }
          }
        }

        img {
          width: 507px;
          height: 507px;
        }
      }
    }

  }


.project-info-spell_group {
  display: flex;
  border: 1px solid rgba(226, 226, 226, 1);
  margin: 0 19px;
  padding: 19px 21px 15px 21px;
  border-top: none;

  .info-left {
    flex: 1.8;
    padding-top: 10px;
    .info-left-first-line {
      font-size: 15px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(238, 38, 51, 1);

      span:nth-child(1) {
        font-size: 36px;
      }
       span:nth-child(2) {
         margin-left 8px;
        font-size:22px;
        font-family:PingFang SC;
        text-decoration:line-through;
        color:rgba(179,179,179,1);
      }

    }

    .info-left-second-line {
      margin-bottom: 10px;
      font-size: 15px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(238, 38, 51, 1);

     text {
        margin-right: 20px;
        font-size: 22px;
        text-decoration: line-through;
        color: rgba(179, 179, 179, 1);
      }

      .group-number {
        padding: 0 10px;
        font-weight: normal;
        border: 1px solid rgba(238, 38, 51, 1);
        border-radius: 13px;
      }
    }

    .info-left-third-line {
      margin-bottom: 13px;
      line-height 28px;
      height: 50px;
      width: 270px;
      font-size: 22px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(0, 0, 0, 1);

      .text {
        width: 280px;
        overflow: hidden;
      }
    }

    .info-left-fourth-line,
    .info-left-fifth-line {
      // width: 100%;
      width: 300px;
      font-size: 16px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(179, 179, 179, 1);

      .text {
        width: 300px;
        overflow: hidden;
      }
    }

  }

  .info-right-spell_group {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 37.6px;
    margin-left 15px

  .info-right-first-line {
    height 142px;
    width 142px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #F7F7F7;
    border-radius: 4px;
    .qr-code {
      width: 122px;
      height 122px;
    }
  }
  .info-right-second-line {
      margin-top 10px;
      font-size: 14px;
      font-family: PingFang SC;
      font-weight: bold;
      color: rgba(85, 85, 85, 1);
    }
  }

}

