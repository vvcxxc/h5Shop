.prize_page {
  width 100vw
  height 100vh
  
  .prize_tab {
    width 100%
    height: 80px
    display: flex
    .prize_label {
      width 50%
      position relative
      text-align: center
      line-height: 80px
      color #FE6E23
    }
    .prize_activity {
      font-weight: bold
    }
    .prize_activity::after {
      content: ''
      display: block
      position: absolute
      width 60px
      height 7px
      border-radius: 4px
      background: #FB8866
      bottom: 0
      left 50%
      transform: translateX(-50%)
    }
  }

}
.list_data {
    min-height: 100%;
    height auto;
    padding 30px 17px 50px 17px;
    background:#FE6E38;
  }

   .user_prompt_box{
     display: flex;
     flex-direction: column;
     align-items: center;
      .user_prompt{
        color: #fff;
        margin-bottom: 20px;
        text-align: center;
      }
      .taro-img {
        height: auto;
        width: 400px;
      }
    }
